cutechess-cli \
        -srand $RANDOM \
        -pgnout games.pgn \
        -repeat \
        -recover \
        -tournament gauntlet \
        -rounds 500000 \
        -concurrency 6 \
        -ratinginterval 50 \
        -draw movenumber=50 movecount=5 score=20 \
        -openings file=$HOME/Chess/books/2moves_v2.pgn format=pgn order=random \
        -engine cmd=$HOME/src/feliscatus/build/bin/Feliscatus name=felix1 tc=40/2+0.05 \
        -engine cmd=$HOME/src/feliscatus/build/bin/Feliscatus name=felix2 tc=40/2+0.05 \
        -each timemargin=60000 option.Hash=512 proto=uci
